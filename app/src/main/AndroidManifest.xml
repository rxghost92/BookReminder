<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookreminder">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/bookremindericon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/bookremindericon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".login.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.search.PreviewFragment" />
        <activity android:name=".MainActivity" />
        <activity android:name=".login.LoginFrameActivity" />
        <activity android:name=".login.Option"></activity>
        <activity android:name=".login.ResetPasswordActivity" />
        <activity android:name=".login.RegisterActivity" />
    </application>

</manifest>